{
  "eslint.options": {
    "extensions": [".js", ".ts", ".tsx"]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,
  "cSpell.words": [
    "Billingcity",
    "Billingpostal",
    "Billingstate",
    "Billingstreet",
    "Mailingcity",
    "Mailingpostalcode",
    "Mailingstate",
    "originalname"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true
}
